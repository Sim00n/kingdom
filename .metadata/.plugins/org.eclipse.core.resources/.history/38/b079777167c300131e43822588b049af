package net.lsrp.kingdom.graphics;

import java.util.Iterator;

public class FontSprite {

	Sprite a = new Sprite(12, 0, 0, SpriteSheet.font);
	Sprite b = new Sprite(12, 0, 1, SpriteSheet.font);
	Sprite c = new Sprite(12, 0, 2, SpriteSheet.font);
	Sprite d = new Sprite(12, 0, 3, SpriteSheet.font);
	Sprite e = new Sprite(12, 0, 4, SpriteSheet.font);
	
	public static String get(String s) {
		
		Iterator i = new Iterator(s);
		
		return "a";
	}
}
